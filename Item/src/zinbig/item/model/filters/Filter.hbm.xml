<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="zinbig.item.model.filters">
	<class name="Filter" table="item_filter">
		<id name="oid" type="string" column="OID_FILTER">
<!--			<generator class="org.hibernate.id.TableHiLoGenerator" />-->
<generator class="assigned"></generator>
		</id>
					
		<property name="name" column="filter_name"></property>
		
		<property name="filterString" length="2000"></property>
		
		<property name="favorite"></property>
		
		<property name="filterComponentByProject"></property>
		
		<property name="filterComponentByState"></property>
		
		<property name="filterComponentByResponsible"></property>
		
		<property name="filterComponentByItemType"></property>
		
		<property name="filterComponentByNode"></property>
		
		<property name="filterComponentByText"></property>
		
		<property name="filterComponentByItemId"></property>
		
		<property name="negateItemType"></property>
		<property name="negateNode"></property>
		<property name="negateProject"></property>
		<property name="negateResponsible"></property>
		<property name="negateState"></property>
		
		
	</class>

</hibernate-mapping>